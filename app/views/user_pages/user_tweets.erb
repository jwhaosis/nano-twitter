<div class="row">
  <div class="col-4">
      <%= erb :'user_pages/profile', :layout => false, :locals => { :user => @searched_user } %>
  </div>

  <div class="col-8">
    <div class="card">
      <ul class="list-group list-group-flush">
        <%= erb :'tweet_pages/tweets_display_helper', :layout => false, :locals => { tweets: @tweets, retweets_hash: @retweets_hash, likes_hash: @likes_hash, user_likes: @user_likes } %>
      </ul>
    </div>
  </div>
</div>
